          <span class="input-group-text acc-label p-0">Personal Info</span>
          <textarea class="form-control w-100 acc-info full_name" aria-label="With textarea" rows="3"></textarea>   
          {% comment %}  {{ customer.name }} {% endcomment %}
        </div>
        <div class="accordion accordion-flush" id="accordionFlushExample_5">
          <div class="accordion-item">
            <h2 class="accordion-header pb-3" id="flush-headingFive">
              <button class="accordion-button p-0 collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseFive" aria-expanded="false" aria-controls="flush-collapseFive">
                {{ 'customer.reset_password.title' | t }}
              </button>
            </h2>
            <div id="flush-collapseFive" class="accordion-collapse collapse" aria-labelledby="flush-headingFive" data-bs-parent="#accordionFlushExample_5">
              <div class="accordion-body p-0 pb-3">
                {% comment %}   {% form 'reset_customer_password' %}

                                  {% if form.errors %}
                                    <div tabindex="-1" data-login-error>{{ form.errors | default_errors }}</div>
                                  {% endif %}

                                  <label for="ResetPassword">
                                    {{ 'customer.reset_password.password' | t }} *
                                  </label>
                                  <input type="password"
                                    name="customer[password]"
                                    id="ResetPassword"
                                    class="w-full isRequired {% if form.errors contains 'password' %}border-scheme-accent{% endif %}">

                                  <label for="PasswordConfirmation">
                                    {{ 'customer.reset_password.password_confirm' | t }} *
                                  </label>
                                  <input type="password"
                                    name="customer[password_confirmation]"
                                    id="PasswordConfirmation"
                                    class="w-full isRequired {% if form.errors contains 'password_confirmation' %}border-scheme-accent{% endif %}">


                                  <div class="row">
                                    <div class="extra-padding pt-4 mt-1">
                                      <input type="submit" class="btn btn-primary" style="cursor: pointer" value="Update">
                                      <input type="button" class="btn btn-primary" style="cursor: pointer" value="Discard changes">
                                    </div>
                                  </div>
                                {% endform %}
                {% endcomment %}
              </div>
            </div>
          </div>
        </div>
        <div class="input-group d-block">
          <span class="input-group-text acc-label p-0">Delivery Address</span>
          <textarea class="form-control w-100 mb-5 acc-address deliveryAddress" aria-label="With textarea"></textarea>
        </div>
        <div class="input-group d-block">
          <span class="input-group-text acc-label p-0">Tell us a safe & sheltered spot to leave your box</span>
          <div class="position-relative acc-edit">
            <textarea type="text" name="textarea" required readonly class="form-control w-100 acc-note" aria-label="With textarea">Please leave at the front door</textarea>
            <input class="position-absolute" name="Edit" type="submit"  style="cursor: pointer" value="Edit">
            <input type="button" class="d-none" style="cursor: pointer" value="Discard changes">
          </div>
        </div>
      </div>
    </div>
  </div>



<div class="section-content py-section-vertical-spacing px-section max-w-site account-NEW d-none" data-color-scheme="primary">
  <div class="mb-10 lg:flex lg:items-center lg:justify-between">
    <h1 class="font-heading heading-template mb-2.5">{{ 'customer.account.title' | t }}</h1>
    <div class="text-sm">
      {% if customer.first_name != blank %}
        {% capture first_name %}<a class="" href="{{ routes.account_url }}">{{ customer.first_name }}</a>{% endcapture %}
        {{ 'layout.customer.logged_in_as_html' | t: first_name: first_name }}
      {% else %}
        <a class="" href="{{ routes.account_url }}">{{ 'layout.customer.account' | t }}</a>
      {% endif %}</span> /
      <a class="inline-block text-primary-accent mb-2" href="{{ routes.account_logout_url }}">{{ 'layout.customer.log_out' | t }}</a>
    </div>
  </div>

  <div class="lg:grid grid-cols-1 lg:grid-cols-12 max-w-screen">
    <div class="lg:col-span-8">
      <h2 class="font-heading heading-secondary mb-2.5">{{ 'customer.orders.title' | t }}</h2>

      {% paginate customer.orders by 20 %}
        {% if customer.orders.size != 0 %}
          <table class="responsive-table text-sm lg:text-base">
            <thead class="text-sm">
              <tr>
                <th scope="col">{{ 'customer.orders.order_number' | t }}</th>
                <th scope="col">{{ 'customer.orders.date' | t }}</th>
                <th scope="col">{{ 'customer.orders.payment_status' | t }}</th>
                <th scope="col">{{ 'customer.orders.fulfillment_status' | t }}</th>
                <th scope="col">{{ 'customer.orders.total' | t }}</th>
              </tr>
            </thead>
            <tbody>
              {% for order in customer.orders %}
                <tr class="responsive-table-row lg:border-none">
                  <td class="text-primary-accent" data-label="{{ 'customer.orders.order_number' | t }}"><span class="underline">{{ order.name | link_to: order.customer_url }}</span></td>
                  <td data-label="{{ 'customer.orders.date' | t }}">{{ order.created_at | date: format: 'month_day_year' }}</td>
                  <td data-label="{{ 'customer.orders.payment_status' | t }}">{{ order.financial_status_label }}</td>
                  <td data-label="{{ 'customer.orders.fulfillment_status' | t }}">{{ order.fulfillment_status_label }}</td>
                  <td data-label="{{ 'customer.orders.total' | t }}">{{ order.total_price | money }}</td>
                </tr>
              {% endfor %}
            </tbody>
          </table>
        {% else %}
          <p>{{ 'customer.orders.none' | t }}</p>
        {% endif %}

        {% if paginate.pages > 1 %}
          {% render 'pagination' with paginate, section_color: 'primary' %}
        {% endif %}

      {% endpaginate %}
    </div>

    <div class="mt-8 lg:mt-0 lg:col-span-3 lg:col-start-10 mb-2">
      <div class="card-container">
        <div class="card p-5 relative z-shape-divider">
          <h2 class="font-heading heading-secondary mb-2.5">{{ 'customer.account.details' | t }}</h2>
          <div class="mb-4">{{ customer.default_address | format_address }}</div>
          {%- capture address_link -%}{{ 'customer.account.view_addresses' | t }} ({{ customer.addresses_count }}){%- endcapture -%}
          <a class="inline-block push-btn w-full theme-button" href="{{ routes.account_addresses_url }}">
            <span class="push-btn-surface w-full">{{ 'customer.account.view_addresses' | t }} ({{ customer.addresses_count }})</span>
          </a>
        </div>
        <div class="card-border"></div>
      </div>
    </div>
  </div>
</div>

